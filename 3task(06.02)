def calc ():
    oper=int(input())
    x=float(input(""))
    y=float(input(""))
    if (oper==1):
        print(x+y)
    if (oper==2):
        print(x*y)
    if (oper==3):
        print(x-y)
    if (oper==4):
        if (x!=0):
            print(x**y)
    if (oper==5):
         if (y!=0):
            print(x/y)
calc()

 
 
    
