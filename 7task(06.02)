#задача7
import math
x=int(input(""))
y=int(input(""))
z=int(input(""))
def dis():
    D=y**2-4*x*z
    if D<0:
        print("Корней нет")
    elif  D==0:
            f1=-y/2*x
            print("Уравнение имеет один корень>>", f1)
    elif  D>0:
            f1=(-y+pow(D))/2*x
            f2=(-y-pow(D))/2*x
            print("Уравнение имеет два корня>>",f1,f2)
dis()

